{
    "lock": false,
    "fmt": {
        "indentWidth": 4,
        "lineWidth": 160,
        "exclude": [
            "td/"
        ]
    },
    "lint": {
        "exclude": [
            "td/"
        ]
    },
    "tasks": {
        "check": "deno cache --check=all *.ts",
        "dev": "deno fmt && deno lint && deno task check",
        "test": "deno test --no-check"
    }
}
